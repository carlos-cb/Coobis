{% extends 'base.html.twig' %}

{% block body %}
	<div class="container">
		<h1>SEO index</h1>

		<ol class="breadcrumb">
			<li><a href="{{ path('homepage') }}">Home</a></li>
			<li><a href="{{ path('data_selectCategory') }}">Category Select</a></li>
			<li class="active">SEO Index</li>
		</ol>
	</div>

	<div class="container">
		<div class="col-lg-3 col-md-4 col-sm-6 col-xs-12 shaixuan">
			<div class="alert" style="background: #dde4e6;">
				<h4>Rank</h4>
				<div slider id="slider-distance">
					<div>
						<div inverse-left style="width:70%;"></div>
						<div inverse-right style="width:70%;"></div>
						<div range style="left:30%;right:0%;"></div>
						<span thumb style="left:30%;"></span>
						<span thumb style="left:100%;"></span>
						<div sign style="left:30%;">
							<span id="value">3</span>
						</div>
						<div sign style="left:100%;">
							<span id="value">10</span>
						</div>
					</div>
					<input id="rank-down" type="range" tabindex="0" value="3" max="10" min="0" step="1" oninput="
					  this.value=Math.min(this.value,this.parentNode.childNodes[5].value-1);
					  var value=(10/(parseInt(this.max)-parseInt(this.min)))*parseInt(this.value)-(10/(parseInt(this.max)-parseInt(this.min)))*parseInt(this.min);
					  var children = this.parentNode.childNodes[1].childNodes;
					  children[1].style.width=value*10+'%';
					  children[5].style.left=value*10+'%';
					  children[7].style.left=value*10+'%';children[11].style.left=value*10+'%';
					  children[11].childNodes[1].innerHTML=this.value;" />

					<input id="rank-up" type="range" tabindex="0" value="10" max="10" min="0" step="1" oninput="
					  this.value=Math.max(this.value,this.parentNode.childNodes[3].value-(-1));
					  var value=(10/(parseInt(this.max)-parseInt(this.min)))*parseInt(this.value)-(10/(parseInt(this.max)-parseInt(this.min)))*parseInt(this.min);
					  var children = this.parentNode.childNodes[1].childNodes;
					  children[3].style.width=(10-value)*10+'%';
					  children[5].style.right=(10-value)*10+'%';
					  children[9].style.left=value*10+'%';children[13].style.left=value*10+'%';
					  children[13].childNodes[1].innerHTML=this.value;" />
				</div>
				<br><br>
				<h4>SubdomainMozRank</h4>
