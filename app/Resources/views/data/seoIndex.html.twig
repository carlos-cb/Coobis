							<a href="#" aria-label="Previous">
								<span aria-hidden="true">&laquo;</span>
							</a>
						</li>
						<li><a href='{{ path('data_seo_page', { 'categoryId': categoryId, 'page': 1 }) }}'>1</a></li>
						<li><a href='{{ path('data_seo_page', { 'categoryId': categoryId, 'page': 2 }) }}'>2</a></li>
						<li><a href='{{ path('data_seo_page', { 'categoryId': categoryId, 'page': 3 }) }}'>3</a></li>
						<li class="disabled">
							<a href="#" aria-label="Next">
								<span aria-hidden="true">&raquo;</span>
							</a>
						</li>
					</ul>
				</div>
			</div>
			<div class="container" id="zong">
				{% for datum in datas %}
					<div id="content" class="row">
						<div class="posts-block col-lg-12 col-md-12 col-sm-12 col-xs-12">
							<article>
								<h3 class="title">{{ datum.url }}</h3>
								<div class="post-content">
									<div class="posts-block col-lg-3 col-md-3 col-sm-3 col-xs-12">
										<div class="round-button"><div class="round-button-circle"><a href="http://{{ datum.mozUrl }}"  target='_blank' class="round-button">Visit!!</a></div></div>
									</div>
									<div class="posts-block col-lg-9 col-md-9 col-sm-9 col-xs-12">
										<div class="cuadro_small autoridad_rank">
											<div class="titulo">
												<b>Rank</b>
											</div>
											<div class="puntuacion_small">
												<b>{{ datum.mozRank }}</b>
											</div>
										</div>
										<div class="cuadro_medio ga subrank">
